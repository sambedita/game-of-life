node('REDHAT'){
    stage('scm'){
       git 'https://github.com/sambedita/game-of-life.git' 

    }

    stage('build'){
        sh label: '', script: '  mvn package'
        
    }

    stage('postbuild'){
        junit 'gameoflife-web/target/surfire-reports/*.xml'
        archiveArtifacts artifacts: 'gameoflife-web/target/*.war'
        
        
    }

}
